{
    "name": "passman",
    "version": "1.0.0",
    "author": "Nizam Chaudhary",
    "description": "Passman monorepo",
    "keywords": [],
    "private": true,
    "license": "ISC",
    "type": "module",
    "scripts": {
        "dev": "nx run-many -t dev --output-style=stream",
        "clean": "nx run-many -t clean --output-style=stream",
        "typecheck": "nx run-many -t typecheck --output-style=stream",
        "cleanAll": "nx run-many -t clean --output-style=stream; rm -rf node_modules; rm -rf apps/*/node_modules",
        "lint": "nx run-many -t lint --output-style=stream",
        "format": "prettier --write .",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@nx/eslint": "21.1.2",
        "@nx/vite": "21.1.2",
        "@nx/web": "21.1.2",
        "@vitest/ui": "^3.2.1",
        "eslint": "^9.28.0",
        "husky": "^9.1.7",
        "jiti": "2.4.2",
        "lint-staged": "^16.1.0",
        "nx": "21.1.2",
        "prettier": "^3.5.3",
        "schema": "^0.2.1",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vitest": "^3.2.1"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
