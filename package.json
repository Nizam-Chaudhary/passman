{
  "name": "passman",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "description": "Passman monorepo",
  "author": "Nizam Chaudhary",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "nx run-many -t dev --output-style=stream",
    "clean": "nx run-many -t clean --output-style=stream",
    "typecheck": "nx run-many -t typecheck --output-style=stream",
    "cleanAll": "nx run-many -t clean --output-style=stream; rm -rf node_modules; rm -rf apps/*/node_modules",
    "lint": "nx run-many -t lint --output-style=stream",
    "lint:fix": "nx run-many -t lint:fix --output-style=stream",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nx/eslint": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/web": "21.1.2",
    "@vitest/ui": "^3.2.1",
    "biome": "^0.3.3",
    "eslint": "^9.28.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "lint-staged": "^16.1.0",
    "nx": "21.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "schema": "^0.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
